<div class="col-md-3">
  <h3 class="page-title mb-5">
    Publish steps
  </h3>
  <div>
    <div class="list-group list-group-transparent mb-0">
      

      <% wizard_steps.each_with_index do |wizard_step, index| %>
    

        <%= link_to wizard_path(wizard_step), class: "list-group-item list-group-item-action d-flex align-items-center #{ step == wizard_step ? 'active' : 'white' }" do %>

          <span class="icon mr-3">
            <i class="fa fa-check"></i>
          </span>

          <%= t(".#{wizard_step}") %>
          
          <span class="ml-auto badge badge-primary">
             kk
          </span>

      <% end %>
      <% end %>


    </div>

  </div>
</div>